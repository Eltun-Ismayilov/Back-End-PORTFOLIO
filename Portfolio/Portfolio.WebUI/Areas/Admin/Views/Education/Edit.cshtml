
@model EducationViewModel

@{
    ViewData["Title"] = "Edit";
}


<h4>Education Edit</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="TimeInterval" class="control-label"></label>
                <input asp-for="TimeInterval" class="form-control" />
                <span asp-validation-for="TimeInterval" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Occoption" class="control-label"></label>
                <input asp-for="Occoption" class="form-control" />
                <span asp-validation-for="Occoption" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CompanyName" class="control-label"></label>
                <input asp-for="CompanyName" class="form-control" />
                <span asp-validation-for="CompanyName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Location" class="control-label"></label>
                <input asp-for="Location" class="form-control" />
                <span asp-validation-for="Location" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="LinkForDiplom" class="control-label"></label>
                <input asp-for="LinkForDiplom" class="form-control" />
                <span asp-validation-for="LinkForDiplom" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="Description" class="control-label"></label>
                <input asp-for="Description" class="form-control" />
                <span asp-validation-for="Description" class="text-danger"></span>
            </div>

            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a class="btn btn-outline-dark" asp-action="Index">Back to List</a>
</div>

